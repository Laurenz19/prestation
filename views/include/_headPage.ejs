                        <li class="sidebar-item  has-sub">
                            <a href="/patient" class='sidebar-link'>
                                <i data-feather="user" width="20"></i>
                                <span>Patient</span>
                            </a>

                            <ul class="submenu ">

                                <li>
                                    <a href="/patient">Liste des Patients</a>
                                </li>

                                <li>
                                    <a href="/new-patient">Ajout</a>
                                </li>

                            </ul>

                        </li>

                        <!-- Traitement-->
                        <li class="sidebar-item  has-sub">
                            <a href="/traitement" class='sidebar-link'>
                                <i data-feather="file-text" width="20"></i>
                                <span>Traitement</span>
                            </a>

                            <ul class="submenu ">

                                <li>
                                    <a href="/traitement">Liste des traitements</a>
                                </li>

                                <li>
                                    <a href="/new-traitement">Ajout</a>
                                </li>
                            </ul>

                        </li>

                        <li class="sidebar-item active ">
                            <a href="/prestation" class='sidebar-link'>
                                <i data-feather="home" width="20"></i>
                                <span>Préstation</span>
                            </a>

                        </li>
                    </ul>
                </div>
                <button class="sidebar-toggler btn x"><i data-feather="x"></i></button>
            </div>
        </div>
